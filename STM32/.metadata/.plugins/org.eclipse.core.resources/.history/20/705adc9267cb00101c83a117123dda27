#include <stdint.h>
#include <stdio.h>
#include <stm32f446xx.h>



int main(void)
{
	// ENABLE GPIOA CLOCK
	GPIOA_CLK_EN();
	GPIOC_CLK_EN();
	// SETTING OUTPUTMODE FOR MODER
	// CLEARING 11 TH AND 10 TH BITS
	GPIOA->MODER &= ~(0x3 << 10);
	// SETTING 10TH BIT FOR GENERAL PURPOSE OUTPUT MODE
	GPIOA->MODER |= (1<<10);
	// SETTING OUTPUT TYPE TO PUSH-PULL
	GPIOA->OTYPER &= ~(1<<5);
	// CLEARING 11TH AND 10TH BITS
	GPIOA->OSPEEDR &= ~(0X3 << 10);
	// SETTING 10TH BIT
	GPIOA->OSPEEDR |= (1<<10);

	GPIOA->PUPDR &= ~(0X3 << 10);

	for(;;)
	{
		GPIOA->ODR ^= (1<<5);
		for(int i=0;i<640000;i++);
	}
}

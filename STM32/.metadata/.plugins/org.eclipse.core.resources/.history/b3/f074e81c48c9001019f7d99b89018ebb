#include <stdint.h>
#include <stdio.h>
#include <stm32f446xx.h>



int main(void)
{
	// ENABLE GPIOA CLOCK
	GPIOA_CLK_EN();
	// SETTING OUTPUTMODE FOR MODER
	GPIOA->MODER = 0;
	// CLEARING 11 TH AND 10 TH BITS
	GPIOA->MODER &= ~(0x3 << 10);
	// SETTING 11TH AND 10TH BITS
	GPIOA->MODER |= (1<<10);
	// SETTING OUTPUT TYPE TO PUSH-PULL
	GPIOA->OTYPER &= (1<<5);

	GPIOA->OSPEEDER &= ~(0X3 << 10);
	GPIOA->OSPEEDER |= (1<<10);
	printf("Hello SWV!\n");
    for(;;);
}
